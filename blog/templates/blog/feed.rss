{# Minified because HTML minifier can not minify XML files #}{% load static %}<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet href="{% static "css/rss-feeds.css" %}" type="text/css"?><rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom"><channel><title>The Blog of Noble Mushtak</title><link>{{ site_domain }}{% url "blog:index" %}</link><atom:link href="{{ site_domain }}{% url "blog:rss_feed" %}" rel="self" type="application/rss+xml"/><description>The latest blog posts from Noble Mushtak.</description>{% for post in posts %}<item><title>{{ post.title }}</title><link>{{ site_domain }}{% url "blog:post" slug=post.slug %}</link><description>{{ post.description }}</description><pubDate>{{ post.pubtime }}</pubDate><guid isPermaLink="false">{{ post.slug }}</guid></item>{% endfor %}</channel></rss>